   "source": [
    "func sum(nums ...int) {\n",
    "    fmt.Print(nums, \" \")\n",
    "    total := 0\n",
    "    for _, num := range nums {\n",
    "        total += num\n",
    "    }\n",
    "    fmt.Println(total)\n",
    "}"
